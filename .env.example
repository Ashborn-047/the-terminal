# The Terminal - Environment Variables Example
# Copy this file to .env and fill in the values

# SpacetimeDB Configuration
# The WebSocket URL for the SpacetimeDB backend
VITE_SPACETIME_HOST=wss://cloud.spacetimedb.com/linux-terminal-academy-dev

# Application Configuration
VITE_APP_NAME="Linux Terminal Academy"

# Error Tracking (Sentry)
# Mock DSN for local development simulation
VITE_SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_DEBUG_MODE=false
